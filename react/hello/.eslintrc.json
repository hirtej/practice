{
  "env" : {
    "browser" : true,
    "es6" : true
  },
  "parserOptions" : {
    "sourceType" : "module",
    "ecmaFeatures" : {
      "experimentalObjectRestSpread" : true,
      "jsx" : true
    }
  },
  "extends" : [ "eslint:recommended", "plugin:react/recommended" ],
  "plugins" : [ "react" ],
  "rules" : {
    "no-console" : "off",
    "semi": ["error", "always"],
    "semi-spacing": ["error", {"after": true, "before": false}],
    "semi-style": ["error", "last"],
    "no-extra-semi": "error",
    "no-unexpected-multiline": "error",
    "no-unreachable": "error"
  }
}
